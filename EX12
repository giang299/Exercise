#include <iostream>
double Power(double x, long n)
{
    double result=1;
    if (n==0) return result;
    do
    {    
          if (n&1)
          { 
            result*=x ;
          }    
          x*=x ;
          n>>=1 ; 
    } while (n);
    return result ;   
}
int main()
{
    double x=5 ;
    std::cout<<" Funtion  :"<<Power(x,5)<<std::endl;
    return 0;
}
