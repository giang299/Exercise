#include <iostream>
using namespace std;
//Loops
double mutilication1(int x){
    for (int i=x; i>1 ; i-=1,x*=i) ;
    return x ; 
}
//Recursive Function
int mutilication2(int x){
   if(x<1) return 1 ;
   return  x*mutilication2(x-1);
}
int main(){
    int x=5 ;
    cout<<"Funtion 1 :"<<mutilication1(x)<<endl;
    cout<<"Funtion 2 :"<<mutilication2(x)<<endl;
  
    return 0;
}
